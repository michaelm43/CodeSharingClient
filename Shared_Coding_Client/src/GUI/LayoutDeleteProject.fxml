<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<BorderPane stylesheets="@LayoutForm.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <VBox fx:id="vboxCheckList" BorderPane.alignment="CENTER">
         <opaqueInsets>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </opaqueInsets>
         <BorderPane.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </BorderPane.margin>
      </VBox>
   </center>
   <bottom>
      <HBox BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#cancel" text="cancel" />
            <Region HBox.hgrow="ALWAYS">
               <cursor>
                  <Cursor fx:constant="H_RESIZE" />
               </cursor>
            </Region>
            <Button id="btnDelete" fx:id="btnDelete" alignment="CENTER_RIGHT" contentDisplay="CENTER" mnemonicParsing="false" onAction="#deleteFiles" text="delete" />
         </children>
      </HBox>
   </bottom>
   <opaqueInsets>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </opaqueInsets>
</BorderPane>
